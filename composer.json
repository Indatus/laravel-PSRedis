{
    "name": "geniuslinchao/laravel-PSRedis",
    "description": "A simple sentinel/redis driver wrapper for laravel.",
    "authors": [
        {
            "name": "Damien Russell",
            "email": "drussell@indatus.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": ">=4.2.18",
        "sparkcentral/psredis": "dev-master"
    },
    "require-dev": {
        "laravel/framework": ">=4.2.18",
        "phpunit/phpunit": ">=4.0",
        "mockery/mockery": ">=0.9"
    },
    "autoload": {
        "classmap": [
            "src/lib"
        ],
        "psr-0": {
            "Indatus\\LaravelPSRedis\\": "src/"
        }
    },
    "psr-4": {
        "": "src/lib"
    },
    "minimum-stability": "stable",
